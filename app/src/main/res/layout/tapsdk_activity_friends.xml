<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/init_cn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="国内初始化" />

            <Button
                android:id="@+id/init_io"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="海外初始化" />

            <Spinner
                android:id="@+id/set_lang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>


            <Button
                android:id="@+id/login_tap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="tap登录" />

        </LinearLayout>

        <com.facebook.login.widget.LoginButton
            android:id="@+id/login_fackbook"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="30dp" />

        <com.linecorp.linesdk.widget.LoginButton
            android:id="@+id/login_line"
            android:layout_width="match_parent"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="30dp"
            android:layout_height="wrap_content" />

        <EditText
            android:id="@+id/userIdTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="先获取用户信息"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <Button
                android:id="@+id/getProfileButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="获取用户信息"
                />
            <Button
                android:id="@+id/anonyLogin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="匿名登录"
                />
        </LinearLayout>

        <EditText
            android:id="@+id/friendIdEditText"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:hint="输入好友id"
            />
        <Button
            android:id="@+id/onlineButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="上线" />
        <Button
            android:id="@+id/addFriendButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="添加好友" />

        <Button
            android:id="@+id/deleteFriendButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="删除好友"/>

        <Button
            android:id="@+id/getFollowingList"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="获取好友列表"
            />

        <Button
            android:id="@+id/friendInviteButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="获取分享链接"
            />
        <Button
            android:id="@+id/checkButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="检查是否是好友"
            />
        <Button
            android:id="@+id/offlineButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="下线"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <EditText
                android:id="@+id/typeEditTextView"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:hint="输入key"
                />
            <EditText
                android:id="@+id/valueEditTextView"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:hint="输入值"
                />
            <Button
                android:id="@+id/submitRichPresenceButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="设置富信息"
            />
        </LinearLayout>

    <LinearLayout
        android:id="@+id/richPresenceLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <Button
            android:id="@+id/allRequest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="打印未处理申请"/>
        <Button
            android:id="@+id/all_decline_request"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="打印已拒绝申请"/>
    </LinearLayout>

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/requestid"
            android:hint="输入申请用户ID"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/acceptRequest"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="接受申请"/>
            <Button
                android:id="@+id/declineRequest"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="拒绝申请"/>
            <Button
                android:id="@+id/deleteRequest"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="删除申请"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <EditText
                android:minWidth="200dp"
                android:id="@+id/input_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="输入用户名"/>
            <Button
                android:id="@+id/searchButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="查找用户"/>

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <EditText
                android:id="@+id/rich_key1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="input key1"
                android:minWidth="100dp"/>
            <EditText
                android:id="@+id/rich_value1"
                android:hint="input value1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:minWidth="100dp"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <EditText
                android:id="@+id/rich_key2"
                android:hint="input key2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minWidth="100dp"/>
            <EditText
                android:id="@+id/rich_value2"
                android:hint="input value2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:minWidth="100dp"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <EditText
                android:id="@+id/rich_key3"
                android:hint="input key3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minWidth="100dp"/>
            <EditText
                android:id="@+id/rich_value3"
                android:hint="input value3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:minWidth="100dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
        <Button
            android:id="@+id/multiRich"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="批量设置富信息"/>

        <Button
            android:id="@+id/multiClearRich"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="批量清除富信息"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/queryTapFriend"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="查询Tap好友" />

            <Button
                android:id="@+id/queryNextTapFriend"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="查询下一页" />

            <Button
                android:layout_marginStart="30dp"
                android:id="@+id/queryTapFriendWithoutCache"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="查询Tap好友(不使用缓存)" />

        </LinearLayout>

        <EditText
            android:id="@+id/et_friend_id"
            android:layout_marginTop="10dp"
            android:hint="TDS好友的objectId"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:minWidth="100dp"/>

        <Button
            android:id="@+id/followTapUser"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="在Tap中关注该用户"/>

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/et_find_short"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        <Button
            android:id="@+id/bt_search_by_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="好友码查找好友"/>

        <Button
            android:layout_marginStart="30dp"
            android:id="@+id/bt_add_by_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="好友码添加好友"/>
        </LinearLayout>

        <Button
            android:layout_marginStart="30dp"
            android:id="@+id/bt_query_fackbook"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="查询Fackbook好友"/>

    </LinearLayout>
</ScrollView>