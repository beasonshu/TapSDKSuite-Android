<?xml version="1.0" encoding="utf-8"?>
<com.tapsdk.suite.internal.TouchBarView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >
    <FrameLayout
        android:id="@+id/touchFrameLayout"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        >
        <ImageView
            android:id="@+id/touchImageView"
            android:layout_width="6dp"
            android:layout_height="36dp"
            android:src="@drawable/tapsdk_ic_expand_components"
            android:scaleType="center"
            android:layout_gravity="center_vertical"
        />

        <ImageView
            android:id="@+id/senseToolImageView"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="center_vertical|right"
            android:src="@drawable/tapsdk_suite_ic_sensetool"
            android:scaleType="center"
        />

    </FrameLayout>
</com.tapsdk.suite.internal.TouchBarView>
